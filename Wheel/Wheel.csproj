<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <OutputType>Library</OutputType>
    <TargetFramework>net8.0</TargetFramework>
    <ImplicitUsings>enable</ImplicitUsings>
    <Nullable>enable</Nullable>
  </PropertyGroup>

  <PropertyGroup Condition=" '$(Configuration)|$(Platform)' == 'Debug|AnyCPU' ">
    <WarningLevel>4</WarningLevel>
    <AllowUnsafeBlocks>true</AllowUnsafeBlocks>
    <LangVersion>latest</LangVersion>
  </PropertyGroup>
  <PropertyGroup Condition=" '$(Configuration)|$(Platform)' == 'Release|AnyCPU' ">
    <AllowUnsafeBlocks>true</AllowUnsafeBlocks>
    <LangVersion>latest</LangVersion>
    <DebugSymbols>true</DebugSymbols>
    <WarningLevel>4</WarningLevel>
  </PropertyGroup>
  <ItemGroup>
    <None Remove="Crypto\" />
    <None Remove="Crypto\Primitives\" />
    <None Remove="Crypto\Primitives\WordVectors\" />
    <None Remove="Crypto\Primitives\ByteVectors\" />
    <None Remove="Crypto\SHA\" />
    <None Remove="Crypto\Primitives\DWordVectors\" />
    <None Remove="Crypto\Primitives\Miscellaneous\" />
    <None Remove="Crypto\Primitives\QWordVectors\" />
    <None Remove="Crypto\RIPEMD\" />
  </ItemGroup>
  <ItemGroup>
    <Folder Include="Crypto\" />
    <Folder Include="Crypto\Primitives\" />
    <Folder Include="Crypto\Primitives\WordVectors\" />
    <Folder Include="Crypto\Primitives\ByteVectors\" />
    <Folder Include="Crypto\SHA\" />
    <Folder Include="Crypto\Primitives\DWordVectors\" />
    <Folder Include="Crypto\Primitives\Miscellaneous\" />
    <Folder Include="Crypto\Primitives\QWordVectors\" />
    <Folder Include="Crypto\RIPEMD\" />
  </ItemGroup>
</Project>
