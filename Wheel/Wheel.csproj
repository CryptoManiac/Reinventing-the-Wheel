<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <OutputType>Library</OutputType>
    <TargetFramework>net8.0</TargetFramework>
    <ImplicitUsings>enable</ImplicitUsings>
    <Nullable>enable</Nullable>
  </PropertyGroup>

  <PropertyGroup Condition=" '$(Configuration)|$(Platform)' == 'Debug|AnyCPU' ">
    <WarningLevel>4</WarningLevel>
    <AllowUnsafeBlocks>true</AllowUnsafeBlocks>
    <LangVersion>latest</LangVersion>
  </PropertyGroup>
  <PropertyGroup Condition=" '$(Configuration)|$(Platform)' == 'Release|AnyCPU' ">
    <AllowUnsafeBlocks>true</AllowUnsafeBlocks>
    <LangVersion>latest</LangVersion>
    <DebugSymbols>true</DebugSymbols>
    <WarningLevel>4</WarningLevel>
  </PropertyGroup>
  <ItemGroup>
    <None Remove="Crypto\" />
    <None Remove="Crypto\Primitives\" />
    <None Remove="Crypto\Hashing\" />
    <None Remove="Crypto\Hashing\RIPEMD\" />
    <None Remove="Crypto\Hashing\SHA\" />
    <None Remove="Crypto\Primitives\Miscellaneous\" />
    <None Remove="Crypto\Hashing\SHA\SHA-512\" />
    <None Remove="Crypto\Hashing\SHA\SHA-256\" />
    <None Remove="Crypto\Hashing\SHA3\" />
    <None Remove="Crypto\Hashing\HMAC\" />
    <None Remove="Crypto\Hashing\HMAC\SHA2\" />
    <None Remove="Crypto\Hashing\PBKDF\" />
  </ItemGroup>
  <ItemGroup>
    <Folder Include="Crypto\" />
    <Folder Include="Crypto\Primitives\" />
    <Folder Include="Crypto\Hashing\" />
    <Folder Include="Crypto\Hashing\RIPEMD\" />
    <Folder Include="Crypto\Hashing\SHA\" />
    <Folder Include="Crypto\Primitives\Miscellaneous\" />
    <Folder Include="Crypto\Hashing\SHA\SHA-512\" />
    <Folder Include="Crypto\Hashing\SHA\SHA-256\" />
    <Folder Include="Crypto\Hashing\SHA3\" />
    <Folder Include="Crypto\Hashing\HMAC\" />
    <Folder Include="Crypto\Hashing\HMAC\SHA2\" />
    <Folder Include="Crypto\Hashing\PBKDF\" />
  </ItemGroup>
</Project>
